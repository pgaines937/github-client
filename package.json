{
  "name": "github-client",
  "version": "1.0.0",
  "description": "A GitHub Client that uses Electron, React, and Redux, and allows the user to search for a repository",
  "main": "init.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pgaines937/github-client.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "reduxjs",
    "react-router",
    "bootstrap",
    "webpack",
    "flux",
    "jwt",
    "autho0",
    "dashboard",
    "starter kit",
    "boilerplate",
    "create-react-app",
    "websockets",
    "socket.io"
  ],
  "author": "twitter.com/pgaines937 (http://github.com/pgaines937)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgaines937/github-client/issues"
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "browser-sync": "^2.18.8",
    "chai": "^3.5.0",
    "electron": "^1.6.2",
    "electron-builder": "^16.6.1",
    "electron-devtools-installer": "^2.1.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "foreman": "^2.0.0",
    "jest": "^19.0.2",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.8.5",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-virtualized": "^8.11.4",
    "redux": "^3.6.0",
    "redux-form": "^6.6.1",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^1.7.2"
  },
  "scripts": {
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "postinstall": "install-app-deps",
    "develop": "npm run react-build && run-p -r private:watch private:serve",
    "test": "jest --verbose",
    "test:watch": "npm test -- --watchAll",
    "react-start": "react-scripts start",
    "react-test": "react-scripts test --env=jsdom",
    "react-build": "react-scripts build",
    "react-eject": "react-scripts eject",
    "lint": "eslint --no-ignore scripts src test *.js",
    "pack": "run-s private:clean private:compile private:build:all",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "private:build:all": "build -mwl",
    "private:build:mac": "build --mac",
    "private:build:win": "build --win",
    "private:build:linux": "build --linux",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "babel src/ --copy-files --out-dir build",
    "private:clean": "rimraf build"
  }
}
